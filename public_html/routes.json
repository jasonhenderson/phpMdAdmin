{
  "routes": [{
      "path": "/",
      "controller": "HomeController.index",
      "method": "GET"
    }, {
      "path": "/groups",
      "controller": "HomeController.groups",
      "method": "GET"
    }, {
      "path": "/files/:group",
      "controller": "HomeController.files",
      "method": "GET"
    }, {
      "path": "/view/md/:group/:file",
      "controller": "ViewController.md",
      "method": "GET"
    }, {
      "path": "/view/slides/:group/:file",
      "controller": "ViewController.slides",
      "method": "GET"
    }, {
      "path": "/view/pdf/:type/:group/:file/",
      "controller": "ViewController.pdf",
      "method": "GET"
    }, {
      "path": "/edit/:group/:file",
      "controller": "EditController.index",
      "method": "GET"
    }, {
      "path": "/edit/:group/:file",
      "controller": "EditController.index",
      "method": "POST"
    }, {
      "path": "/edit/text/:group/:file",
      "controller": "EditController.text",
      "method": "GET"
    }, {
      "path": "/assets/:group",
      "controller": "AssetsController.index",
      "method": "GET"
    }, {
      "path": "/assets/upload/:group",
      "controller": "AssetsController.upload",
      "method": "GET,POST"
    }, {
      "path": "/api/createGroup",
      "controller": "ApiController.createGroup",
      "method": "POST"
    }, {
      "path": "/api/removeGroup/:group",
      "controller": "ApiController.removeGroup",
      "method": "GET"
    }, {
      "path": "/api/createFile",
      "controller": "ApiController.createFile",
      "method": "POST"
    }, {
      "path": "/api/removeFile/:group/:file",
      "controller": "ApiController.removeFile",
      "method": "GET"
    }
 ]
}